a=int(input())
b=int(input())
c=int(input())
print("Коэффициент a =",a)
print("Коэффициент b =",b)
print("Коэффициент c =",c)
D=b**2-4*a*c
print("Дискриминант D =", D)
if D>0:
    s1=(-b+D**0.5)/(2*a)
    s2=(-b-D**0.5)/(2*a)
    if D==0:
        print('Корень уравнения =', s1)
    else:
        print('Первый корень уравнения =', s1, 'Второй корень уравнения =', s2)
else:
    print("Корней нет")
